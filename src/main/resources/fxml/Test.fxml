<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="TestController">
    <children>
        <TextArea fx:id="textArea" focusTraversable="false" layoutX="18.0" layoutY="177.0" prefHeight="200.0"
                  prefWidth="565.0" promptText="with" wrapText="true"/>
        <Button fx:id="encodeTrigger" focusTraversable="false" layoutX="110.0" layoutY="80.0" mnemonicParsing="false"
                onAction="#triggerEncoding" prefHeight="50.0" prefWidth="113.0" text="ENCODE"/>
        <Button fx:id="triggerDecoding" focusTraversable="false" layoutX="351.0" layoutY="80.0" mnemonicParsing="false"
                onAction="#triggerDecoding" prefHeight="50.0" prefWidth="113.0" text="DECODE"/>
    </children>
</AnchorPane>
